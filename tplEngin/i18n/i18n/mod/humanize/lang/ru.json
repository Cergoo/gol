/*
  языковые ресурсы русскаго языка подключаемого модуля humanize1024
  для пакета github.com/Cergoo/gol/tplEngin/i18n/i18n 
  Единицы измерения, ненужное можно закомментировать
*/

{
"pluralRule": "Ru", 
"plural": {
  "bit":  ["бит", "бит", "бита"],  
  "byte":  ["байт", "байт", "байта"]
//  "metr":  ["метр", "метров", "метра"]
},
"lists": {
  "bitshort": ["б", "Кб", "Мб", "Гб", "Тб", "Пб", "Еб"],
  "byteshort": ["Б", "КБ", "МБ", "ГБ", "ТБ", "ПБ", "ЕБ"],
  "+prefix1000": ["", "кило", "мега", "гига", "тера", "пета", "экcа"],
  "-prefix1000": ["", "мили", "микро", "нано", "пико", "фемто", "атто"]
}
}
